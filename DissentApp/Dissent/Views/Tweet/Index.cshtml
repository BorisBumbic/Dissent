

<style>
    table {
        border-spacing: 0;
        width: 100%;
    }

    td {
        border: solid 1px #ddd;
        padding: 5px 10px;
    }

    .wrapper .map {
        padding: 100px;
        height: 100%;
        width: 100%;
        float: right;
    }
</style>



<script async defer src="https://maps.googleapis.com/maps/api/js?key=AIzaSyDX1XsmTAZIyKquTYmsVwO2mxZS5LZtC6Y"></script>




<div id="app" class="wrapper" >
    <div id="content">
        <div id="map" class="map"></div>

    </div>
    <div id="searchBox">


        Search:<input v-model="userinput" />
        Enter Radius:<input v-model="radius" />
        Latitude:<input v-model="lat" />
        Longitude:<input v-model="lng" />



        <button v-on:click="showTweetSentiment()">Ok</button>

        <table>
            <tr v-for="x in sentimentResponse">
                <td>{{x.text}}</td>
                <td>{{x.sentiment}}</td>

            </tr>
        </table>
    </div>
</div>

<script src="https://cdn.jsdelivr.net/npm/vue/dist/vue.js"></script>
<script src="/js/dissent.js"></script>





